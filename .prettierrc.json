{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": true,
  "tabWidth": 4,
  "bracketSpacing": true,
  "parser": "typescript",

  "importOrder": [
    "^@nestjs/.*", "^(?![./])", "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderParserPlugins" : ["classProperties", "typescript", "decorators-legacy"]
}
